# Expo app (used at runtime)
EXPO_PUBLIC_SUPABASE_URL=
EXPO_PUBLIC_SUPABASE_ANON_KEY=
EXPO_PUBLIC_USE_REMOTE_FEED=true
# Optional override for quote endpoint (defaults to ${EXPO_PUBLIC_SUPABASE_URL}/functions/v1/quote)
EXPO_PUBLIC_QUOTES_ENDPOINT=

# Seed script (service role recommended)
SUPABASE_SERVICE_ROLE_KEY=
# Optional override for scripts; defaults to EXPO_PUBLIC_SUPABASE_URL when set
SUPABASE_URL=

# Edge Function scraper env (server-only)
SCRAPER_TIMEOUT_MS=
SCRAPER_USER_AGENT=
SCRAPER_BATCH_SIZE=
SCRAPER_STORAGE_STATE=
SCRAPER_DISABLE_HTTP2=
SCRAPER_NAV_WAIT_UNTIL=
SCRAPER_POST_NAV_WAIT_MS=
SCRAPER_BLOCK_RESOURCES=
