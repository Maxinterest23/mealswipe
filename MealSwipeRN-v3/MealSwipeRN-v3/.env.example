# Expo app (used at runtime)
EXPO_PUBLIC_SUPABASE_URL=
EXPO_PUBLIC_SUPABASE_ANON_KEY=
EXPO_PUBLIC_USE_REMOTE_FEED=true
# Optional override for quote endpoint (defaults to ${EXPO_PUBLIC_SUPABASE_URL}/functions/v1/quote)
EXPO_PUBLIC_QUOTES_ENDPOINT=

# Seed script (service role recommended)
SUPABASE_SERVICE_ROLE_KEY=
# Optional override for scripts; defaults to EXPO_PUBLIC_SUPABASE_URL when set
SUPABASE_URL=

# Edge Function provider env (server-only)
PROVIDER_BASE_URL=
PROVIDER_API_KEY=
# Optional if provider needs custom host header (Apify: api.apify.com)
PROVIDER_HOST=
# Apify actor id (e.g. apify~e-commerce-scraping-tool)
PROVIDER_ACTOR_ID=
# Optional provider timeout in ms (defaults to 60000)
PROVIDER_TIMEOUT_MS=
# Optional: include extra Apify fields (defaults to false)
PROVIDER_INCLUDE_EXTRA=
# Optional: batch size for Apify detail URLs (defaults to 3)
PROVIDER_BATCH_SIZE=
# Optional: Apify dataset id to ingest (scheduled runs)
PROVIDER_DATASET_ID=
